function str(String) {
    if (typeof String == "string"){                              //  1. Скрипт должен на вход принимать строку.
        console.log("ето строка, уже неплохо")                   //  2. После проверки строки выводить в консоль сообщение где будет конкретно написано, что не так в ведённой строке.
    } else console.log("енто не строка")
    if (String.length <= 5){                                     //  3. Минимум 5 символов в строке
        console.log("мало буков, нужно 5 минмум")                           
    }
    if (String.length >= 64){                                    //  4. Максимум 64 символа в строке
        console.log("но буков многовато, 64 максимум")
    }
    if (/[A-Za-zА-Яа-я]/.test(String)){                          //  5. В строке должны быть буквы
        console.log("спасибо что буквы хотя бы есть")
    } else console.log("а где буквы в принципе?")
    if (/[A-ZА-Я]/.test(String)) {                               //  6. Должна быть хотя бы одна буква в верхнем регистре
        console.log("спасибо за буквы верхнего регистра")
    } else console.log("и где буквы верхнего регистра?")
    if (/[0-9]/.test(String)) {                                  //  7. Должна быть хотя бы одна цифра
        console.log("спасибо что есть цифры")
    } else console.log("че там с цифрами?")
    if (String.includes("@")) {                                  //  8. Должна быть хотя бы одна @
        console.log("собака в наличии")
    } else console.log("добавь собачку")
    if (!String) {                                               //  9. Строка не должна быть пустой
        console.log("напиши уже хоть что-то")
    }
}
str("")
